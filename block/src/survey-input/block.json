{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "beastfeedbacks/survey-input",
	"version": "0.1.0",
	"title": "survey-input",
	"category": "beastfeedbacks",
	"icon": "thumbs-up",
	"description": "アンケート用の入力項目",
	"supports": {
		"html": false
	},
	"attributes": {
		"content": {
			"type": "string",
			"source": "text",
			"selector": "label",
			"default": "サンプル入力値"
		}
	},
	"textdomain": "beastfeedbacks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
